    "helmet": "^3.15.0",
    "heroku": "^7.47.2",
    "jsonwebtoken": "^8.5.0",
    "knex": "^0.21.12",
    "morgan": "^1.9.1",
    "pg": "^8.4.2",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^1.18.9",
